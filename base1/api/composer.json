{
    "name": "myvendor/my-project",
    "description": "A Slim Framework application with Eloquent ORM",
    "require": {
        "php": "^7.3",
        "slim/slim": "^4.0",
        "slim/psr7": "^1.3",
        "illuminate/database": "^8.0",
        "illuminate/events": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\Controllers\\": "api/controllers/",
            "App\\": "api/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
